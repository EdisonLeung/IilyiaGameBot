{
  "name": "iilyiagamebot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "del /q dist",
    "build": "esbuild src/index.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist/index.js",
    "deploy": "npm run build && aws lambda update-function-code --function-name DiscordBot-Iilya --zip-file \"fileb://dist/index.zip\"",
    "postbuild": "cd dist && 7z a -tzip index.zip index.js*",
    "register": "node dist/commands.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "esbuild": "^0.23.1"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "discord.js": "^14.16.1",
    "tweetnacl": "^1.0.3"
  }
}
